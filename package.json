{
  "name": "twindr",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.2.8",
  "workspaces": [
    "apps",
    "server/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "turbo": "^2.5.0",
    "prettier": "^3.5.3",
    "typescript": "5.8.2",
    "@types/amqplib": "^0.10.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/nanoid": "^3.0.0",
    "@types/pg": "^8.11.13",
    "@types/react-dom": "^19",
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.744.0",
    "@grpc/grpc-js": "^1.13.2",
    "@grpc/proto-loader": "^0.7.13",
    "amqplib": "^0.10.7",
    "google-auth-library": "^9.15.1",
    "hono": "^4.7.5",
    "ioredis": "^5.6.1",
    "kafkajs": "^2.2.4",
    "mongoose": "^8.13.2",
    "nanoid": "^5.1.5",
    "next": "15.2.4",
    "pg": "^8.14.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.3"
  }
}
